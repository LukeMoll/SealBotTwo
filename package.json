{
  "name": "sealbottwo",
  "version": "1.0.0",
  "description": "Flexible Discord bot",
  "main": "bot.js",
  "scripts": {
    "test": "npm start",
    "start": "node .",
    "init-config": "npm run config-backup && cp config-example.yaml config.yaml && nano -ET4 config.yaml && echo \"To make more changes, edit config.yaml\"",
    "config-backup": "cp config.yaml config.yaml.backup 2> /dev/null || :",
    "add": "node add.js"
  },
  "author": "Luke Moll",
  "repository": {
    "type": "git",
    "url": "https://github.com/LukeMoll/sealbottwo.git"
  },
  "license": "BSD-3-Clause",
  "dependencies": {
    "discord.js": "^11.3.2",
    "gamedig": "^1.0.46",
    "js-yaml": "^3.12.0",
    "mojang-api": "0.0.2",
    "nedb": "^1.8.0"
  }
}
